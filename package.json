{
  "name": "nuxt-ava-e2e-unit-testing",
  "version": "1.0.0",
  "description": "Nuxt demo application with Ava, running E2E and Unit tests",
  "author": "Vinayak Kulkarni",
  "private": true,
  "scripts": {
    "dev": "nuxt",
    "build": "nuxt build",
    "start": "nuxt start",
    "generate": "nuxt generate",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "precommit": "npm run lint",
    "test:unit": "TEST=unit nyc ava ./test/specs/*.spec.js --verbose --color",
    "test:e2e": "TEST=e2e nyc ava ./test/e2e/*.js --verbose --color",
    "test": "npm run test:unit & npm run test:e2e"
  },
  "dependencies": {
    "cross-env": "^5.2.0",
    "@nuxtjs/axios": "^5.4.1",
    "@nuxtjs/pwa": "^3.0.0-beta.16",
    "element-ui": "^2.8.2",
    "nuxt": "^2.7.1"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.4.4",
    "@babel/register": "^7.4.4",
    "@nuxtjs/eslint-config": "^0.0.1",
    "@vue/test-utils": "^1.0.0-beta.29",
    "ava": "^1.4.1",
    "babel-eslint": "^10.0.1",
    "babel-plugin-module-resolver": "^3.2.0",
    "browser-env": "^3.2.6",
    "eslint": "^5.16.0",
    "eslint-config-prettier": "^4.3.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-loader": "^2.1.2",
    "eslint-plugin-import": "^2.17.2",
    "eslint-plugin-node": "^9.0.1",
    "eslint-plugin-nuxt": "^0.4.3",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-promise": "^4.1.1",
    "eslint-plugin-standard": "^4.0.0",
    "eslint-plugin-vue": "^5.2.2",
    "jsdom-global": "^3.0.2",
    "nodemon": "^1.19.0",
    "nyc": "^14.1.1",
    "prettier": "^1.17.1",
    "require-extension-hooks": "^0.3.3",
    "require-extension-hooks-babel": "^1.0.0",
    "require-extension-hooks-vue": "^3.0.0"
  }
}
